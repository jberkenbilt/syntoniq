; Sample syntoniq score to test parsing
version(1)

; Define a scale  
scale(name="12-EDO", divisions=[12, 2, 1], base_pitch=*220*^3|12)

; Note lines for different parts
[violin.1] 2:c' e' g' 1:c''
[violin.2] 4:c e g | 2:d f a  
[viola.1] 1:c, g, e | 2:c g

; Dynamic lines
[violin] 60@0 80@1/4 100@1/2< 60@1
[viola] 40@0 60@1/4 80@1/2> 40@1
syntoniq(version=1)

define_scale(name="5-EDO") <<
  ^0|5 p
  ^1|5 q
  ^2|5 r
  ^3|5 s
  ^4|5 t
>>

tune(scale="5-EDO" part="p1")
[p1.0] 1:p q r
[p1.1] 1:q s t
[p1.2] 1:r t p'

tune(scale="default" part="p1")
[p1.0]  1:c d e f g a b c'
[p1.1]  1/2:e g f a g c d c e c f d g f 1:e
[p1.2]  2:c, f,2 g,2 1:g, c,
